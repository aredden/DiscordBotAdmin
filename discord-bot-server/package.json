{
	"name": "discord-bot-server",
	"version": "0.0.1",
	"description": "Server for DiscordBot.ts to run on.",
	"main": "index.js",
	"scripts": {
		"clean": "rimraf dist/*",
		"copy-assets": "ts-node tools/copyAssets",
		"lint": "tslint -c tslint.json -p tsconfig.json --fix",
		"tsc": "tsc",
		"build": "npm-run-all clean lint tsc",
		"dev:start": "npm-run-all build start",
		"dev": "nodemon --watch src -e ts --exec npm run dev:start",
		"start": "node dist/index.js",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"author": "Alex Redden",
	"license": "ISC",
	"devDependencies": {
		"@types/body-parser": "^1.17.1",
		"@types/express": "^4.17.2",
		"@types/fs-extra": "^8.0.1",
		"@types/node": "^13.7.0",
		"@types/shelljs": "^0.8.6",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"fs-extra": "^8.1.0",
		"nodemon": "^2.0.2",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.1",
		"shelljs": "^0.8.3",
		"ts-node": "^8.6.2",
		"tslint": "^6.0.0",
		"typescript": "^3.7.5",
		"@types/cors": "^2.8.6",
		"@types/socket.io": "^2.1.4",
		"discord.js-typings": "github:discordjs/discord.js-typings",
		"eslint": "^6.8.0"
	},
	"dependencies": {
		"@discordjs/uws": "^10.149.0",
		"axios": "^0.19.2",
		"body-parser": "^1.19.0",
		"chalk": "^3.0.0",
		"cors": "^2.8.5",
		"discord.js": "^12.x",
		"moment": "^2.24.0",
		"mongodb": "^3.5.2",
		"socket.io": "^2.3.0",
		"winston": "^3.2.1",
		"npm": ">=6.14.6"
	}
}
